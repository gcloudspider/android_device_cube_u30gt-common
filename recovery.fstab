/data			ext4			/dev/block/mtd/by-name/data
/cache			ext4			/dev/block/mtd/by-name/cache
/intsd			datamedia		/dev/null
/sdcard			vfat			/dev/block/mmcblk0p1	/dev/block/mmcblk0
